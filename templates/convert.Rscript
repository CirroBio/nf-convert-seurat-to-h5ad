#!/usr/bin/env Rscript

library(Seurat)
library(SeuratDisk)

print("Reading in ${seurat_file}")
obj = readRDS("${seurat_file}")
print(obj)

print("Saving in h5Seurat format")
SaveH5Seurat(obj, filename = "${sample_id}.h5Seurat")
print("Saving in h5ad format")
Convert("${sample_id}.h5Seurat", dest = "h5ad")
